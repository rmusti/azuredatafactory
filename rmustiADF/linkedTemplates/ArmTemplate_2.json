			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/triggerYellowTaxI')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "copyDataEventTrig",
							"type": "PipelineReference"
						},
						"parameters": {
							"srcfolderName": "[parameters('triggerYellowTaxI_properties_copyDataEventTrig_parameters_srcfolderName')]",
							"srcfileName": "[parameters('triggerYellowTaxI_properties_copyDataEventTrig_parameters_srcfileName')]"
						}
					}
				],
				"type": "BlobEventsTrigger",
				"typeProperties": {
					"blobPathBeginsWith": "/adftutorial/blobs/yellowtaxi/",
					"ignoreEmptyBlobs": true,
					"scope": "[parameters('triggerYellowTaxI_properties_typeProperties_scope')]",
					"events": [
						"Microsoft.Storage.BlobCreated"

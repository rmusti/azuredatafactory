{
	"name": "ADXIngestAppTables",
	"properties": {
		"activities": [
			{
				"name": "ADX Load App Tables",
				"type": "AzureDataExplorerCommand",
				"dependsOn": [
					{
						"activity": "ADX Load Master Table",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
